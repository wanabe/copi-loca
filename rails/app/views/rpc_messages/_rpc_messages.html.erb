<table id="<%= limit == 5 ? 'latest_5_rpc_messages' : 'rpc_messages' %>" class="rpc-messages-table">
  <thead>
    <tr>
      <th>ID</th>
      <th>Type</th>
      <th>Dir</th>
      <th>Rpc id</th>
      <th>Method</th>
      <th>Params</th>
      <th>Result</th>
      <th>Error</th>
    </tr>
  </thead>
  <tbody data-controller="limit-list" data-limit-list-max-value="<%= limit %>" data-limit-list-order-value="prepend">
    <% rpc_messages.each do |rpc_message| %>
      <%= render rpc_message %>
    <% end %>
  </tbody>
</table>