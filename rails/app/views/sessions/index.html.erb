<p style="color: green"><%= notice %></p>

<% content_for :title, "Sessions" %>

<h1>Sessions</h1>

<table id="sessions">
  <%= render "table_header" %>
  <tbody>
    <% @sessions.each do |session| %>
      <%= render "table", session: session %>
    <% end %>
  </tbody>
</table>

<%= link_to "New session", new_session_path %>
