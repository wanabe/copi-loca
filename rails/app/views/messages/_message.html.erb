<div id="<%= dom_id message %>">
  <div>
    <strong>Time:</strong>
    <%= message.created_at.strftime("%Y-%m-%d %H:%M:%S.%L") %>
  </div>
  <div>
    <strong>Direction:</strong>
    <%= message.direction %>
  </div>

  <div>
    <strong>Content:</strong>
    <%= message.content %>
  </div>

</div>
